{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/_next/static/*",
      "/images/*",
      "/favicon.ico",
      "/*.css",
      "/*.js"
    ]
  },
  "routes": [
    {
      "route": "/*",
      "serve": "/index.html",
      "statusCode": 200
    }
  ],
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html",
      "statusCode": 200
    }
  }
} 